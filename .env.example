ENABLE_AUTH=false
# JWT secret key for signing tokens
JWT_SECRET=your_jwt_secret_here

# Token names
JWT_TOKEN_NAME=jwtToken
NEXT_JWT_TOKEN_NAME=front-end-server-jwtToken

# API URL (choose the one you need and comment out others)
NEXT_PUBLIC_API_URL=http://localhost:8080
# NEXT_PUBLIC_API_URL=http://192.168.100.10:8080
# NEXT_PUBLIC_API_URL=https://your-production-api-url.com

# Local R2 bucket (public access)
NEXT_PUBLIC_R2_PUBLIC_URL=https://your-local-r2-public-url.com

# Production R2 bucket
# NEXT_PUBLIC_R2_PUBLIC_URL=https://your-production-r2-public-url.com
